@font-face {
    font-family: 'Roboto';
    src: url('<%= font_path('roboto/Roboto-Thin-webfont.eot') %>');
    src: url('<%= font_path('roboto/Roboto-Thin-webfont.eot') %>?#iefix') format('embedded-opentype'),
         url('<%= font_path('roboto/Roboto-Thin-webfont.woff') %>') format('woff'),
         url('<%= font_path('roboto/Roboto-Thin-webfont.ttf') %>') format('truetype'),
         url('<%= font_path('roboto/Roboto-Thin-webfont.svg') %>#Roboto') format('svg');
    font-weight: 100;
    font-style: normal;
}
