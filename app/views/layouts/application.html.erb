<!DOCTYPE html>
<html>
  <head>
    <title> <%= controller_name.humanize || "MunicipeCrud" %> </title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'layouts/common/navigation/navbar' %>
    <div class="wrapper">
      <%= render 'layouts/common/partials/notices' %>
      <%= yield %>
    </div>
    <%= javascript_include_tag 'application' %>
  </body>
</html>
